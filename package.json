{
  "name": "resume-optimizer",
  "version": "1.0.0",
  "description": "AI-powered resume optimization application using Gemini AI",
  "main": "index.js",
  "scripts": {
    "build": "cd frontend && npm install --force && npm run build",
    "dev": "cd frontend && npm run dev",
    "preview": "cd frontend && npm run preview",
    "setup": "npm run setup:frontend && npm run setup:functions",
    "setup:frontend": "cd frontend && npm install",
    "setup:functions": "cd netlify/functions && npm install"
  },
  "dependencies": {
    "lambda-multipart-parser": "^1.0.1",
    "jspdf": "^2.5.1",
    "docx": "^8.5.0",
    "html-pdf-node": "^1.0.8",
    "@google/generative-ai": "^0.21.0",
    "cheerio": "^1.0.0",
    "axios": "^1.7.7",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.8.0"
  },
  "keywords": [
    "resume",
    "optimization",
    "ai",
    "gemini",
    "ats",
    "job",
    "application"
  ],
